{
  "profiles": {
    "msdos": {
      "command": {
        "cmd": "dosbox",
        "args": [
          "-c",
          "@echo off",
          "-exit",
          "-c",
          "cls",
          "{executable}"
        ],
        "evars": {
          "LD_PRELOAD": "/usr/local/lib/libshimmer_sdl.so"
        }
      },
      "executables": [
        ".*\\.(exe|bat|conf)$"
      ],
      "features": [
        {
          "pattern": ".*\\.(exe|bat|conf)$"
        }
      ]
    },
    "gog-dosbox": {
      "command": {
        "cmd": "dosbox",
        "args": [
          "{executable}"
        ],
        "evars": {
          "LD_PRELOAD": "/usr/local/lib/libshimmer_sdl.so"
        }
      },
      "executables": [
        ".*\\.(exe|bat|conf)$"
      ],
      "features": [
        {
          "pattern": ".*\\.(exe|bat|conf)$"
        },
        {
          "pattern": "dosbox",
          "weight": 2
        },
        {
          "pattern": ".*_(single|setup).conf$",
          "weight": 2
        }
      ]
    },
    "gog-scummvm": {
      "command": {
        "cmd": "scummvm",
        "args": [
          "-c",
          "{executable}"
        ],
        "evars": {}
      },
      "executables": [
        ".*\\.ini$"
      ],
      "features": [
        {
          "pattern": ".*\\.ini$"
        },
        {
          "pattern": "scummvm",
          "weight": 2
        }
      ]
    },
    "mednafen": {
      "command": {
        "cmd": "mednafen",
        "args": [
          "{executable}"
        ],
        "evars": {
          "LD_PRELOAD": "/usr/local/lib/libshimmer_sdl.so"
        }
      },
      "executables": [
        ".*\\.(nes|bin|sms|smc|pce)$"
      ],
      "features": [
        {
          "pattern": ".*\\.(nes|bin|sms|smc|pce)$"
        }
      ]
    },
    "gzdoom": {
      "command": {
        "cmd": "gzdoom",
        "args": [
          "-iwad",
          "{executable}"
        ],
        "evars": {
          "DRI_PRIME": "1"
        }
      },
      "executables": [
        ".*\\.wad$"
      ],
      "features": [
        {
          "pattern": ".*\\.wad$"
        }
      ]
    }
  },
  "extractors": {
    "zip": {
      "extensions": [
        "zip",
        "sh"
      ],
      "command": {
        "cmd": "unzip",
        "args": [
          "{archive}",
          "-d",
          "{target}"
        ]
      }
    },
    "7z": {
      "extensions": [
        "7z"
      ],
      "command": {
        "cmd": "7z",
        "args": [
          "x",
          "-o{target}",
          "{archive}"
        ]
      }
    },
    "innoextract": {
      "extensions": [
        "exe"
      ],
      "command": {
        "cmd": "innoextract",
        "args": [
          "-e",
          "-m",
          "{archive}"
        ]
      }
    },
    "fallback": {
      "command": {
        "cmd": "cp",
        "args": [
          "{archive}",
          "{target}"
        ]
      },
      "extensions": []
    }
  }
}